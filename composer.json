{
    "name": "koba/toolbox-api-client",
    "description": "A PHP wrapper for the toolbox API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wouter Henderickx",
            "email": "wouter.henderickx@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*",
        "php-http/discovery": "^1.19",
        "json-mapper/json-mapper": "^2.21",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Koba\\ToolboxClient\\": "src/ToolboxClient/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
